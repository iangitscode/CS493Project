{
  "name": "CS493 Project",
  "version": "0.1",
  "description": "Generates an interactive transcript for videos on your page",
  "manifest_version": 2,
  "permissions": ["activeTab", "tabs"],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "css": ["styles.css"]
  }],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
