{
  "name": "CS493 Project",
  "version": "0.1",
  "description": "Generates an interactive transcript for videos on your page",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "background": {
    "scripts": ["content.js"]
  },
  "browser_action": {},
  "permissions": ["activeTab", "storage"]
}
